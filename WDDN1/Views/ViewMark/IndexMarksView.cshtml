@model WDDN1.Models.Mark
@model IEnumerable<WDDN1.Models.Mark>
@{
    ViewBag.Title = "IndexMarksView";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>IndexMarksView</h2>
<h2>Index</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.course.CourseName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.student.Fullname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.marks)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.course.CourseName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.student.Fullname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.marks)
            </td>
           
        </tr>
    }

</table>
